#include<iostream>
#include<fstream>
#include<string>
using namespace std;

int main()
{  
    // create a stream for the file
    ifstream inFile("Ch8_Ex4Data.txt")
    string data = "";
    // extract data separated by ','
    while(getline(inFile, data, ',')){
        cout << data << endl;
}
return 0;
}
